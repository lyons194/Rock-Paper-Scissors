if 1:
		a = 0
		b = {}
		while a < 3:
			r = input("Rock, Paper, or Scissors?: ")
			if r == "Rock":
				import random
				guess1 = random.random()
				if guess1 < 0.3333:
					print(" ")
					print(" ")
					print("Computer plays: Rock.")
					print(" ")
					print("---------------> Tie.  Neither party wins this round.")
					print(" ")
					print(" ")
					a = a + 1
				elif 0.3334< guess1 < 0.6666:
					print(" ")
					print(" ")
					print("Computer plays: Paper.")
					print(" ")
					print("---------------> Paper beats Rock.  Computer wins this round.")
					print(" ")
					print(" ")
					a = a + 1
				elif guess1 > 0.6667:
					print(" ")
					print(" ")
					print("Computer plays: Scissors.")
					print(" ")
					print("---------------> Rock beats Scissors.  You win this round.")
					print(" ")
					print(" ")
					a = a + 1
