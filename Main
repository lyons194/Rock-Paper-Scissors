if 1:
	a = 0
	b = {}
	while a < 3:
		r = input("Rock, Paper, or Scissors?: ")
		if r == "Rock":
			import random
			guess1 = random.random()
			if guess1 < 0.3333:
				print("Rock.")
				a = a + 1
			elif 0.3334< guess1 < 0.6666:
				print("Paper.")
				a = a + 1
			elif guess1 > 0.6667:
				print("Scissors.")
				a = a + 1
